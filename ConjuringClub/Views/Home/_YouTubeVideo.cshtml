@using ConjuringClub.ViewModels;

@model YouTubeVideoViewModel

@{
    ConjuringClub.ViewModels.HFrameViewModel hFrame = new ConjuringClub.ViewModels.HFrameViewModel();
    hFrame.Src = "//www.youtube.com/embed/" + Model.VideoId + "?rel=0";
    hFrame.FigCaption = Model.VideoTitle;
    hFrame.AspectRatioWxH = "16x9";
}

@Html.Partial("_hFrame", hFrame)
