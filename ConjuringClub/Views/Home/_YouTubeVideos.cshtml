@model string[]

<section>

    <div class="page-header">
        <h1 id="videos">Videos</h1>
    </div>

    <div class="row">

        @for (var i = 0; i < Model.Length; ++i)
        {
            <div class="col-md-6">
                @Html.Partial("_YouTubeVideo", Model[i])
            </div>
        }

    </div>

</section>