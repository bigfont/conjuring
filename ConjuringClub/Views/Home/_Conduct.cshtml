<!--Conduct-->
<section ng-controller="ConductCtrl">
    <div class="page-header">
        <h1 id="conduct">Code of Engagement</h1>
    </div>
    <ul class="list-unstyled">
        <li ng-repeat="aspiration in aspirations"><strong>{{ aspiration.Notion }}</strong> <p>{{ aspiration.Example }}</p></li>
    </ul>
    <p><span class="label label-warning">Warning</span> If you break the code of engagement, it isn't the end of the world.</p>
</section>
<!--End Conduct-->
