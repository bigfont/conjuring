@model ConjuringClub.ViewModels.OneDriveDocumentViewModel[]

<section>

    <div class="page-header">
        <h1 id="documents">Documents</h1>
    </div>

    <div class="row">

        @for (var i = 0; i < Model.Length; ++i)
        {
            <div class="col-sm-6">
                @Html.Partial("_OneDriveDocument", Model[i])
            </div>
        }

    </div>

</section>