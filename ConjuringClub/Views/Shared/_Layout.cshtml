<!DOCTYPE html>
<html ng-app="conjuringApp">
<head>
    <title>Salt Spring Conjuring Club</title>

    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <meta name="description" content="Salt Spring Conjuring Club - Fridays from 3:30 to 5 pm at the Public Library." />
    <meta name="keywords" content="Magic, Tricks, Card Tricks, Sleight-of-Hand, Salt Spring, Salt Spring Island" />

    <link href="//netdna.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet" type="text/css" />

    @Styles.Render("~/bundles/css")

</head>

<body class="container">

    @Html.Partial("_Header")

    <div class="alert alert-info" role="alert"><strong>Yonkers!</strong> The site takes 30 seconds to load; we have yet to optimize document loading. <i>Our apologies for the wait.</i></div>

    <div class="row">

        @RenderBody()

    </div>

    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.10/angular.js"></script>
    <script src="//angular-ui.github.io/bootstrap/ui-bootstrap-tpls-0.11.0.js"></script>
    <script type="text/javascript" src="//code.jquery.com/jquery-2.1.1.min.js"></script>
    <script type="text/javascript" src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>

    @Scripts.Render("~/bundles/javascript")

</body>
</html>
